[Unit]
Description=SteamOS kdump loader boot-time service

[Service]
Type=oneshot
StandardOutput=journal
ExecStartPre=/usr/lib/kdump/kdump_load.sh
ExecStart=/usr/lib/kdump/submitter_load.sh
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
